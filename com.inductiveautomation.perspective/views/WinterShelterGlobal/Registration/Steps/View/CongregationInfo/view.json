        "config": {
          "expression": "{view.params.congregationCity} !\u003d \u0027\u0027 \u0026\u0026 !isNull({view.params.congregationCity})\r\n\u0026\u0026\r\n{view.params.locationCity} !\u003d \u0027\u0027 \u0026\u0026 !isNull({view.params.locationCity})"
        },
