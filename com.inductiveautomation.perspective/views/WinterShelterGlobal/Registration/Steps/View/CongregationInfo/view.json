        "config": {
          "expression": "{view.params.congregationAddressLine1} !\u003d \u0027\u0027 \u0026\u0026 !isNull({view.params.congregationAddressLine1})\r\n\u0026\u0026\r\n{view.params.locationAddressLine1} !\u003d \u0027\u0027 \u0026\u0026 !isNull({view.params.locationAddressLine1})"
