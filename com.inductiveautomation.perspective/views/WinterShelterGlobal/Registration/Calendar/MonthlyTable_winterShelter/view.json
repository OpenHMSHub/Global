              "config": {
                "expression": "isGood({view.custom.month})"
              },
              "transforms": [
                {
                  "code": "\tenabled \u003d self.view.params.enabled\n\tstart \u003d system.date.getDayOfYear(self.view.custom.start)\n\tend \u003d system.date.getDayOfYear(self.view.custom.end)\n\tstartYear \u003d self.view.custom.StartYear\n\tendYear \u003d self.view.custom.EndYear\n\tmonth \u003d self.view.custom.month\n\tparams \u003d {\n\t  \"dayOfYear\": 0,\n\t  \"label\": \"\",\n\t  \"date\": \"\",\n\t  \"blocked\": False,\n\t  \"important\": False,\n\t  \"selected\": False,\n\t  \"enabled\":enabled\n#\t  ,\"PrevHostDays\":[]\n\t  ,\"RemoveSelection\":False\n\t  ,\"limitDate\":False\n\t  ,\"addition\": False\n\t  ,\"holidays\": False\n\t}\n\tdaysHosting \u003d list(self.view.params.daysHosting)\n\tPrevHostingDays \u003d list(self.view.params.PrevHostDays)\n\tCanceledSchedule \u003d list(self.view.params.CanceledSchedule)\n\tsystem.perspective.print(PrevHostingDays)\n\timportantDays \u003d list(self.view.params.importantDays)\n\tholidaysDates \u003d list(self.view.params.holidaysDates)\n\tmonthStart \u003d system.date.getDate(self.view.custom.year,self.view.custom.month,1)\n\tmonthEnd \u003d system.date.addMonths(monthStart,1)\n\tdayOfYear \u003d system.date.getDayOfYear(monthStart)\n\tdayOfWeek \u003d system.date.getDayOfWeek(monthStart)\n\tdays \u003d system.date.daysBetween(monthStart, monthEnd)\n\taddedDays \u003d list(self.view.params.addedDays)\n\tinstances \u003d []\n\t#Insert blanks until reach first day of month\n\tfor i in range(1, dayOfWeek): \t\t\n\t\tinst \u003d dict(params)\n\t\tinst[\u0027label\u0027] \u003d \u0027\u0027\n\t\tinst[\u0027dayOfYear\u0027] \u003d 0\n\t\tinst[\u0027date\u0027] \u003d \"\"\n\t\tinstances.append(inst)\n\t#Add month days\n\tfor i in range(1,days+1):\n\t\tinst \u003d dict(params)\n\t\tinst[\u0027label\u0027] \u003d i\n\t\tinst[\u0027dayOfYear\u0027] \u003d dayOfYear\n\t\t# get date from dayOfYear\n\t\tif month \u003e 9:\n\t\t\tdate \u003d system.date.getDate(int(startYear), month, i)\n\t\telse:\n\t\t\tdate \u003d system.date.getDate(int(endYear), month, i)\n\t\tinst[\u0027date\u0027] \u003d date\n\t\tif dayOfYear \u003c start and dayOfYear \u003e end: inst[\u0027blocked\u0027] \u003d True\n\t\tif dayOfYear in daysHosting:\n\t\t\tinst[\u0027selected\u0027] \u003d True\n\t\telse:\n\t\t\tsystem.perspective.print(PrevHostingDays)\n\t\t\tif (dayOfYear in PrevHostingDays) or (dayOfYear in CanceledSchedule):\n\t\t\t\tinst[\u0027RemoveSelection\u0027] \u003d True\n\t\tif dayOfYear in importantDays: inst[\u0027important\u0027] \u003d True \n\t\tif dayOfYear in holidaysDates: inst[\u0027holidays\u0027] \u003d True\n\t\tfrom datetime import datetime, date\n\t\tday_of_year_limit \u003d date(2022, 10, 31).timetuple().tm_yday\n \t\tday_of_year \u003d date(2022, 11, 30).timetuple().tm_yday\n#\t\tday_of_year \u003d datetime.now().timetuple().tm_yday\n\t\tif day_of_year \u003e day_of_year_limit:\n\t\t\tinst[\u0027limitDate\u0027] \u003d True\n\t\telse:\n\t\t\tinst[\u0027limitDate\u0027] \u003d False\n\t\tif dayOfYear in addedDays: \n\t\t\tinst[\u0027addition\u0027] \u003d True\n\t\tinst[\u0027PrevHostDays\u0027] \u003d PrevHostingDays\n\t\tinst[\u0027CanceledSchedule\u0027] \u003d CanceledSchedule\n\t\tinstances.append(inst)\n\t\tdayOfYear +\u003d 1\n\treturn instances",
                  "type": "script"
                }
              ],
